separator_block_width=20
command=$HOME/dotfiles/.i3/$BLOCK_NAME

[pacman]
interval=1800

[brightness]
instance=/usr/bin/light
interval=1

[volume]
interval=1

[wireless]
instance=wlp3s0
interval=1

[weather]
instance=SBFL
interval=1800

[battery]
interval=30

[date]
command=date '+%A, %-d de %B, %R'
interval=1
